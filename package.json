{
  "name": "Interference",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "description": "An HTML5 multiplayer snakes-game based on Lance",
  "main": "main.js",
  "dependencies": {
    "@babel/core": "^7.3.4",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.7.0",
    "express": "^4.14.0",
    "lance-gg": "file:lance-gg",
    "query-string": "^4.2.3",
    "socket.io": "^2.1.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-watch": "^7.0.0",
    "babelify": "^7.3.0",
    "eslint": "^3.16.1",
    "eslint-config-google": "^0.7.1"
  },
  "scripts": {
    "start": "babel-node main.js --presets env",
    "start-debug": "babel-node main.js --inspect-brk --presets env",
    "start-dev": "babel-watch main.js --presets env",
    "nodemon": "nodemon main.js --exec babel-node --presets env",
    "postinstall": "webpack --display-error-details",
    "build": "webpack; babel-node main.js --presets env",
    "dev": "webpack --watch",
    "stats": "webpack --env production --profile --json > stats.json"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
